#include <iostream>
using namespace std;
int main()
{
      for (int i=1;i<=5;i++)
      {
          //for space 
          for (int j=1;j<=6-i-1;j++)
          {
              cout<<" ";
          }
          
          for (int j=1;j<=i;j++)
          {
              cout<<"*";
          }
          
          for (int j=2;j<=i;j++)
          {
              cout<<"*";
          }
          cout<<endl;
      }
    return 0;
}
